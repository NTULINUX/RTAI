+ * this program; if not, write to the Free Software Foundation, Inc., 59 Temple
+ * Place - Suite 330, Boston, MA 02111-1307, USA.
+ */
+
+#ifndef _ASM_ARCH_RTAI_TIMER_H_
+#define _ASM_ARCH_RTAI_TIMER_H_
+
+#include <rtai.h>
+
+#include <mach/hardware.h>
+#include <asm/arch/rtai_arch.h>
+
+extern notrace unsigned long long __ipipe_get_tsc(void);
+extern notrace void __ipipe_set_tsc(unsigned long long value);
+
+extern int (*extern_timer_isr)(struct pt_regs *regs);
+extern int rtai_timer_handler(struct pt_regs *regs);
+
+extern unsigned int rt_periodic;
+
+#define DA8XX_TIMER64P0_BASE	0x01c20000
+#define DA8XX_TIMER64P1_BASE	0x01c21000
+#define TIMER64P0_BASE_VIRT	IO_ADDRESS(DA8XX_TIMER64P0_BASE)
+#define TIMER64P1_BASE_VIRT	IO_ADDRESS(DA8XX_TIMER64P1_BASE)
+
+#define TIMER_BASE_VIRT 	TIMER64P1_BASE_VIRT
+
+#define REG_TIM12		(TIMER_BASE_VIRT + 0x10)
+#define REG_TIM34		(TIMER_BASE_VIRT + 0x14)
+#define REG_PRD12		(TIMER_BASE_VIRT + 0x18)
+#define REG_PRD34		(TIMER_BASE_VIRT + 0x1c)
+#define REG_TCR 		(TIMER_BASE_VIRT + 0x20)
+
+#define REG_TIM 		REG_TIM34
+#define REG_PRD 		REG_PRD34
+
+#define TCR_ENAMODE_ONESHOT	0x1
+#define TCR_ENAMODE_PERIODIC	0x2
+#define TCR_ENAMODE_MASK	0x3
+
+#define ENAMODE_SHIFT12 	6
+#define ENAMODE_SHIFT34 	22
+
+#define ENAMODE_SHIFT		ENAMODE_SHIFT34
+
+#define tcr_modemasked		(tcr & ~(TCR_ENAMODE_MASK << ENAMODE_SHIFT))
+
+static inline int rtai_timer_irq_ack(void)
+{
+	unsigned int tcr;
+	unsigned long flags;
+
+	flags = rtai_critical_enter(NULL);
+
+	__ipipe_set_tsc(__ipipe_get_tsc() + __raw_readl(REG_PRD));
+
+	if (!rt_periodic) {
+		tcr = __raw_readl(REG_TCR);
+		__raw_writel(tcr_modemasked, REG_TCR);
+		__raw_writel(tcr_modemasked | (TCR_ENAMODE_ONESHOT << ENAMODE_SHIFT), REG_TCR);
+	}
+
+	rtai_critical_exit(flags);
+
+	return 0;
+}
+
+static inline RTIME rtai_rdtsc(void)
+{
+	return __ipipe_get_tsc() + __raw_readl(REG_TIM);
+}
+
+static inline void rt_set_timer_delay(unsigned long delay)
+{
+	if (delay)
+		__ipipe_mach_set_dec(delay);
+
+	return;
+}
+
+#endif /* !_ASM_ARCH_RTAI_TIMER_H_ */
