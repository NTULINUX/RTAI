index 0000000..9926445
--- /dev/null
+++ b/base/arch/arm/hal/mach-davinci/davinci-timer.c
@@ -0,0 +1,127 @@
+/*
+ * mach-davinci/davinci-timer.c
+ *
+ * ARM/Omap-L138 specific timer
+ * This file is based on mach-at91/at91-timer.c
+ *
+ * Copyright (C) 2013 Torsten Koschorrek (devel@koschorrek.com)
+ *
+ * This program is free software; you can redistribute it and/or modify it under
+ * the terms of the GNU General Public License as published by the Free Software
+ * Foundation, Inc., 675 Mass Ave, Cambridge MA 02139, USA; either version 2 of
+ * the License, or (at your option) any later version.
+ *
+ * This program is distributed in the hope that it will be useful, but WITHOUT
+ * ANY WARRANTY; without even the implied warranty of MERCHANTABILITY or FITNESS
+ * FOR A PARTICULAR PURPOSE.  See the GNU General Public License for more
+ * details.
+ *
+ * You should have received a copy of the GNU General Public License along with
+ * this program; if not, write to the Free Software Foundation, Inc., 59 Temple
+ * Place - Suite 330, Boston, MA 02111-1307, USA.
+ */
+
+#include <asm/arch/rtai_timer.h>
+
+unsigned int rt_periodic;
+EXPORT_SYMBOL(rt_periodic);
+
+static int saved_tcr = 0;
+static int saved_prd = 0;
+
+/* timer_period = COUNTER_VAL/24MHz = 1s/1000/1000*x = x us */
+#define COUNTER_VAL		(0x016e3600/1000/1000*500)
+
+int hw_setup_timer(int period, int mode)
+{
+	int tcr = __raw_readl(REG_TCR);
+
+	__raw_writel(tcr_modemasked, REG_TCR);
+	__raw_writel(0, REG_TIM);
+	__raw_writel(period, REG_PRD);
+	__raw_writel(tcr_modemasked | (mode << ENAMODE_SHIFT), REG_TCR);
+
+	return tcr;
+}
+
+int rt_request_timer (void (*handler)(void), unsigned tick, int use_apic)
+{
+	unsigned long flags;
+
+	printk("timer request: %s mode (period: %i ns)\n",
+	       tick?"periodic":"oneshot",
+	       (tick?tick:COUNTER_VAL)/(RTAI_TSC_FREQ/1000000)*1000);
+
+	printk("timer request: irq=%i base=0x%p tim=0x%p prd=0x%p shift=%i\n",
+	       RTAI_TIMER_IRQ, TIMER_BASE_VIRT, REG_TIM, REG_PRD, ENAMODE_SHIFT);
+
+	flags = rtai_critical_enter(NULL);
+
+	__ipipe_mach_timerstolen = 1;
+
+	rt_times.tick_time = rtai_rdtsc();
+	rt_times.linux_tick = __ipipe_mach_ticks_per_jiffy;
+	saved_prd = __raw_readl(REG_PRD);
+	if (tick > 0) {
+		rt_periodic = 1;
+
+		if (tick > __ipipe_mach_ticks_per_jiffy) {
+			tick = __ipipe_mach_ticks_per_jiffy;
+		}
+		rt_times.intr_time = rt_times.tick_time + tick;
+		rt_times.linux_time = rt_times.tick_time + rt_times.linux_tick;
+		rt_times.periodic_tick = tick;
+		saved_tcr = hw_setup_timer(tick, TCR_ENAMODE_PERIODIC);
+	} else {
+		rt_periodic = 0;
+
+		rt_times.intr_time = rt_times.tick_time + rt_times.linux_tick;
+		rt_times.linux_time = rt_times.tick_time + rt_times.linux_tick;
+		rt_times.periodic_tick = rt_times.linux_tick;
+		saved_tcr = hw_setup_timer(COUNTER_VAL, TCR_ENAMODE_ONESHOT);
+		rt_set_timer_delay(rt_times.periodic_tick);
+	}
+
+	rt_release_irq(RTAI_TIMER_IRQ);
+	rt_request_irq(RTAI_TIMER_IRQ, (rt_irq_handler_t)handler, NULL, 0);
+	extern_timer_isr = rtai_timer_handler;
+
+	rtai_critical_exit(flags);
+
+	return 0;
+}
+
+void rt_free_timer (void)
+{
+	unsigned long flags;
+
+	rt_periodic = 0;
+	__ipipe_mach_timerstolen = 0;
+
+	hw_setup_timer(saved_prd, (saved_tcr >> ENAMODE_SHIFT) & TCR_ENAMODE_MASK);
+	rt_set_timer_delay(__ipipe_mach_ticks_per_jiffy);
+	rt_release_irq(RTAI_TIMER_IRQ);
+	rtai_save_flags_and_cli(flags);
+	extern_timer_isr = NULL;
+	rtai_restore_flags(flags);
+
+	printk("timer freed\n");
+}
+
+int rtai_calibrate_TC (void)
+{
+	unsigned long flags;
+	RTIME t, dt;
+	int i;
+
+	flags = rtai_critical_enter(NULL);
+	rt_set_timer_delay(LATCH);
+	t = rtai_rdtsc();
+	for (i = 0; i < 10000; i++) {
+		rt_set_timer_delay(LATCH);
+	}
+	dt = rtai_rdtsc() - t;
+	rtai_critical_exit(flags);
+
+	return rtai_imuldiv(dt, 100000, RTAI_CPU_FREQ);
+}
